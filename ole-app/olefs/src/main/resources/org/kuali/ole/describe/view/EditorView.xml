                <bean parent="Uif-SecondaryActionButton-Small" p:actionScript="addEInstance()" p:actionEvent="addEInstance" p:actionLabel="E" p:title="Add Eholdings" p:align="left" p:componentSecurity.performActionAuthz="true"/>
