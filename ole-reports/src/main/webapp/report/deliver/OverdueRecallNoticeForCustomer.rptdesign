            <xml-property name="queryText"><![CDATA[select 
IF(ODI.LOCATION IS NULL OR ODI.LOCATION = '', IF(ODH.LOCATION IS NULL,'',ODH.LOCATION ), ODI.LOCATION) as 'Location',
IF(ODI.CALL_NUMBER IS NULL OR ODI.CALL_NUMBER = '', IF(ODH.CALL_NUMBER IS NULL,'',ODH.CALL_NUMBER), ODI.CALL_NUMBER) as 'Call Number',
ODR.ITM_ID as 'Barcode',
ODBI.TITLE as 'Title',
concat(OPEV.FIRST_NM,' ',OPEV.LAST_NM) as 'Patron Name',
OPEV.PTRN_BRCD as 'Patron Barcode',
ODL.CURR_DUE_DT_TIME as 'Recall Due Date'
from 
OLE_DLVR_RQST_T as ODR,
OLE_DS_ITEM_T as ODI,
OLE_DLVR_ITEM_AVAIL_STAT_T as ODIAS,
OLE_DS_HOLDINGS_T as ODH,
OLE_DS_BIB_INFO_T as ODBI,
OLE_PTRN_ENTITY_V as OPEV,
OLE_DLVR_LOAN_T as ODL
where
(ODR.OLE_RQST_TYP_ID = 1 or ODR.OLE_RQST_TYP_ID = 2)
and
ODR.ITM_ID = ODI.BARCODE
and
ODI.ITEM_STATUS_ID = ODIAS.ITEM_AVAIL_STAT_ID
and
ODIAS.ITEM_AVAIL_STAT_CD = 'LOANED'
and
ODI.HOLDINGS_ID =  ODH.HOLDINGS_ID
and
ODBI.BIB_ID = ODH.BIB_ID
and 
ODL.OLE_PTRN_ID = OPEV.OLE_PTRN_ID
and
ODL.ITM_ID = ODR.ITM_ID
and
(OPEV.EMAIL = null or OPEV.EMAIL = '')
and
DATEDIFF(CURDATE(), ODL.CURR_DUE_DT_TIME) > 0]]></xml-property>
